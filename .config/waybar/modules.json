{
  "custom/spacer": {
    "format": ""
  },

  "custom/launcher": {
    "format": "󱓞  Launcher",
    "on-click": "wofi --show drun",
    "tooltip": false
  },

  "sway/workspaces": {
    "all-outputs": true
  },

  "sway/mode": {
    "format": "{} Mode"
  },

  "sway/window": {
    "format": "{title}",
    "tooltip": false,
    "icon": true,
    "icon-size": 14
  },

  "wireplumber": {
    "format": "{icon}    {volume}%",
    "format-muted": "",
    "on-click": "helvum",
    "scroll-step": 2,
    "format-icons": ["", "", ""]
  },

  "tray": {
    "icon-size": 14,
    "spacing": 10
  },

  "clock": {
    "interval": 5,
    "tooltip": true,
    "format": "    {:%r}"
  }
}
